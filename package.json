{
  "name": "language-trainer",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "turbo dev --parallel --filter={next-app,@lt/api}",
    "web": "turbo run dev --filter next-app",
    "api": "cross-env NO_D1_WARNING=true turbo run dev --filter @lt/api",
    "build": "turbo run build --filter=@t4/ui",
    "build:web": "turbo run build --filter=next-app",
    "generate": "pnpm --filter=@lt/api generate",
    "migrate:local": "pnpm --filter=@lt/api migrate:local",
    "check-deps": "check-dependency-version-consistency .",
    "check-types": "tsc --noEmit",
    "fix": "manypkg fix",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@manypkg/cli": "^0.21.0",
    "@supabase/supabase-js": "^2.32.0",
    "check-dependency-version-consistency": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "node-gyp": "^9.4.0",
    "prettier": "^3.0.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.16.1",
    "pnpm": ">=8.6.6"
  },
  "packageManager": "pnpm@8.6.10"
}
